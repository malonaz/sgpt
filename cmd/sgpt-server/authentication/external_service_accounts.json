local roles_config = import 'roles.json';

local service_accounts = [
  // Cli service account.
  {
    "id": "sgpt",
    "role_ids": [role.id for role in roles_config.roles],
  },
];


{
  service_accounts: [
    service_account + { type: "SERVICE_ACCOUNT_TYPE_API_KEY" }
    for service_account in service_accounts
  ]
}
